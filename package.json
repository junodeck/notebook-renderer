{
  "name": "@junodeck/notebook-renderer",
  "version": "0.1.0",
  "description": "React components for rendering parsed Jupyter notebooks",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "private": true,
  "files": [
    "dist"
  ],
  "style": "dist/style.css",
  "scripts": {
    "build": "npm run clean && npm run build:ts && npm run build:css",
    "build:ts": "tsc",
    "build:css": "cp src/style.css dist/style.css",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "type-check": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "author": "FedeB91",
  "license": "ISC",
  "keywords": [
    "jupyter",
    "notebook",
    "react",
    "presentation",
    "data-science",
    "components"
  ],
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "dependencies": {
    "clsx": "^2.0.0",
    "prismjs": "^1.29.0"
  },
  "devDependencies": {
    "@types/prismjs": "^1.26.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "typescript": "^5.0.0"
  }
}
